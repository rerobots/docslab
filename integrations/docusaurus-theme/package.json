{
  "name": "docslab-docusaurus",
  "version": "0.2.9",
  "description": "Docusaurus theme to support docslab-enabled code examples",
  "keywords": [
    "docs",
    "documentation",
    "docusaurus",
    "editor",
    "hardware"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rerobots/docslab.git"
  },
  "scripts": {
    "build": "tsc --build",
    "pub": "test -f lib/index.js && yarn publish",
    "clean": "rm -r -f lib tsconfig.client.tsbuildinfo"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "docslab": "^0.3.12"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@docusaurus/theme-classic": "^2.4.1",
    "@tsconfig/docusaurus": "^2.0.0",
    "@types/react": "^18.2.20",
    "typescript": "^5.1.6"
  }
}
